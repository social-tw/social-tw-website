    "main": "index.js",
    "license": "MIT",
    "private": true,
    "workspaces": [
        "packages/*"
    ],
    "scripts": {
        "install": "lerna bootstrap",
        "build": "lerna run build",
        "circuits": "yarn workspace @unirep-app/circuits run",
