{
    "compilerOptions": {
        "jsx": "react",
        "target": "es2018",
        "module": "commonjs",
        "strict": true,
        "esModuleInterop": true,
        "noImplicitThis": false,
        "resolveJsonModule": true,
        "noImplicitAny": false,
        "declaration": true,
        "skipLibCheck": true,
        "baseUrl": "src",
        "paths": {
            "@/*": ["./*"]
        }
    },
    "exclude": ["**/node_modules"]
}
